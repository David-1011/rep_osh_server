{
    "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "endOfLine": "auto"
            }
        ],
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-method-use-this": "off",
        "no-undef": "warn",
        "consistent-return": "off",
        "no-else-return": "off",
        "no-plusplus": "off",
        "camelcase": "warn"
    }
}